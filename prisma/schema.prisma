generator client {
  provider = "prisma-client-js"
}

// TODO: Production um dia só não vai ser hoje:
// datasource db {
//   provider = "postgresql"
//   url      = env("DATABASE_URL")
// }

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model contato {
  id    Int     @id @default(autoincrement())
  nome  String?
  sobrenome String?
  email String?
  telefone String?
  servico String?
  mensagem String?
}